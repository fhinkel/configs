[alias]
  ci = commit
  s = status
  st = stash
  sp = stash pop
  f = fetch -p
  r = rebase
  ri = rebase -i
  p = push
  pu = "!git push -u origin $(git rev-parse --abbrev-ref HEAD)"
  co = checkout
  up = pull --rebase
  lg = log --oneline
  last = log -1 HEAD
  c = log -p -w --reverse @{u}..
  b = branch -vv
  fix = commit --amend --no-edit
  efix = commit --amend
  d = diff
  ds = diff --staged
  dw = diff --color-words
  a = commit -a --amend --no-edit
  cf = cl format
  cu = cl upload --gerrit
  new = new-branch --upstream-current

[pull]
	rebase = true
